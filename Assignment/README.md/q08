#include<stdio.h>
int main()
{
	int n,min,i;
	printf("Enter the size of array");
	scanf("%d",&n);
	int arr[n];
	printf("Enter the array elements");
	for(i=0;i<n;i++)
	{
		scanf("%d",&arr[i]);
	}
	min=arr[0];
	for(i=0;i<n;i++)
	{
		if(arr[i]<min)
		{
			min=arr[i];
			printf("%d ",min);
		}
	}
	return 0;
}
